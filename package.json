{
  "name": "git_lintcommit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "commit":"git-cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      
    }
  },
  "dependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1"
  },
  "devDependencies": {
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^8.0.3",
    "init": "0.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
